{
    "name": "runroom/samples-bundle",
    "type": "symfony-bundle",
    "description": "Add a render event before rendering templates",
    "keywords": [
        "samples",
        "twig",
        "symfony"
    ],
    "homepage": "https://github.com/runroom/RunroomSamplesBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Runroom",
            "email": "software@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "a2lix/translation-form-bundle": "^3.0",
        "doctrine/doctrine-bundle": "^2.1",
        "egulias/email-validator": "^2.1.16 || ^3.0",
        "friendsofsymfony/ckeditor-bundle": "^1.2 || ^2.2",
        "gedmo/doctrine-extensions": "^2.4 || ^3.0",
        "hubspot/hubspot-php": "^3.0",
        "knplabs/doctrine-behaviors": "^2.0.6",
        "runroom-packages/form-handler-bundle": "^0.11",
        "runroom-packages/render-event-bundle": "^0.11",
        "runroom-packages/sortable-behavior-bundle": "^0.11",
        "sonata-project/admin-bundle": "^3.100 || ^4.0@dev",
        "sonata-project/doctrine-extensions": "^1.9",
        "sonata-project/doctrine-orm-admin-bundle": "^3.34 || ^4.0@dev",
        "sonata-project/media-bundle": "^3.24 || ^4.0@dev",
        "symfony/config": "^4.4 || ^5.3",
        "symfony/dependency-injection": "^4.4 || ^5.3",
        "symfony/event-dispatcher": "^4.4 || ^5.3",
        "symfony/event-dispatcher-contracts": "^1.1 || ^2.0",
        "symfony/form": "^4.4 || ^5.3",
        "symfony/http-foundation": "^4.4.7 || ^5.3",
        "symfony/http-kernel": "^4.4.13 || ^5.3.5",
        "symfony/options-resolver": "^4.4 || ^5.3",
        "symfony/translation": "^4.4 || ^5.3",
        "symfony/validator": "^4.4 || ^5.3",
        "symfony/yaml": "^4.4 || ^5.3",
        "twig/twig": "^2.12 || ^3.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.2",
        "friendsofphp/php-cs-fixer": "^3.0",
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-doctrine": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpstan/phpstan-strict-rules": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "phpunit/phpunit": "^9.3",
        "psalm/plugin-phpunit": "^0.16",
        "psalm/plugin-symfony": "^2.0",
        "runroom-packages/testing": "^0.11",
        "symfony/phpunit-bridge": "^5.3",
        "vimeo/psalm": "^4.3",
        "weirdan/doctrine-psalm-plugin": "^1.0",
        "zenstruck/foundry": "^1.10"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Runroom\\SamplesBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Runroom\\SamplesBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "changelog": [
            "changelog-linker dump-merges --in-categories --ansi",
            "changelog-linker link --ansi",
            "changelog-linker cleanup --ansi"
        ],
        "php-cs-fixer": "php-cs-fixer fix --ansi --verbose",
        "phpstan": "phpstan analyse --no-progress",
        "psalm": "psalm --stats"
    }
}
