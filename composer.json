{
    "name": "runroom/samples-bundle",
    "type": "symfony-bundle",
    "description": "Add a render event before rendering templates",
    "keywords": [
        "samples",
        "twig",
        "symfony"
    ],
    "homepage": "https://github.com/runroom/RunroomSamplesBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Runroom",
            "email": "software@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "a2lix/translation-form-bundle": "^2.4",
        "doctrine/doctrine-bundle": "^2.0",
        "friendsofsymfony/ckeditor-bundle": "^1.2 || ^2.0",
        "gedmo/doctrine-extensions": "^2.4",
        "knplabs/doctrine-behaviors": "^2.0",
        "runroom-packages/form-handler-bundle": "^0.3",
        "runroom-packages/render-event-bundle": "^0.3",
        "runroom-packages/sortable-behavior-bundle": "^0.3",
        "sonata-project/admin-bundle": "^3.61",
        "sonata-project/doctrine-extensions": "^1.5",
        "sonata-project/media-bundle": "^3.23",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/event-dispatcher": "^4.4 || ^5.0",
        "symfony/event-dispatcher-contracts": "^1.1 || ^2.0",
        "symfony/form": "^4.4 || ^5.0",
        "symfony/http-foundation": "^4.4.7 || ^5.0.7",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "symfony/translation": "^4.4 || ^5.0",
        "symfony/validator": "^4.4 || ^5.0",
        "symfony/yaml": "^4.4 || ^5.0",
        "twig/twig": "^2.12 || ^3.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.2",
        "friendsofphp/php-cs-fixer": "^2.16",
        "jangregor/phpstan-prophecy": "^0.6",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-doctrine": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "phpunit/phpunit": "^9.1",
        "symfony/phpunit-bridge": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "conflict": {
        "phpunit/phpunit": "9.1.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Runroom\\SamplesBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Runroom\\SamplesBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "php-cs-fixer": "php-cs-fixer fix --ansi --verbose",
        "phpstan": "phpstan analyse --level=5 --no-progress src tests"
    }
}
